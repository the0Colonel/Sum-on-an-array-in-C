#include<stdio.h>
 
  int main()
   {
   	   int ARR[10],i,n,s=0; 
   	   
   	    do
   	    {
   	    	 printf("Give Number of elements : "); 
   	    	 scanf("%d",&n);
		   }while( n>10 || n<0); 
		   
		    for (i=0;i<n;i++)
		    {
		    	  printf("Give element %d : ",i+1); 
		    	  scanf("%d",&ARR[i]);
		    	 
		    	
			}
   	           
				 for(i=0;i<n;i++)
				 {
				 	 printf("Element %d : %d\n ",i+1,ARR[i]);
				 	
				  } 
   	                
   	                for(i=0;i<n;i++)
   	                 s=s+ARR[i]; 
   	                 
   	                 printf("Sum of array is %d",s);
   	 
   }
